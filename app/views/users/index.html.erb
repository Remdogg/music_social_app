<h1>Users</h1>
<table class = "table">
  <thead>
      <tr>
        <th>Name</th>
        <th>
          Location
        </th>
        <th>
          
        </th>
      </tr>
  </thead>
  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><a href='users/<%=user.id%>'><%= user.first_name%> <%= user.last_name%></a>
      </td>
      <td>
        <%=user.city%>, <%=user.state%>
      </td>
      <td>
         <%= render 'shared/stats' %>
      </td>
    </tr>
  <% end %>
  </tbody>
</table>
